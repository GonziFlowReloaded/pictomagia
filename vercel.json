{
    "version":2,
    "builds":[
        {
            "src":"/app.py",
            "use":"@vercel/python",
            "config":{
                "runtime": "python"
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/app.py"
        }
    ]
    
    
}